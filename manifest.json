{
"manifest_version": 2,

"name": "Skip Price Filter",
"short_name": "SPF",
"description": "Filter items on SkipTheDishes that are cheaper or more than a certain amount.",
"version": "1.0.0",
"default_locale": "en",
"author": "JustSayBoot",
"permissions": [
  "storage"
],
"icons": {
  "16": "img/icon16.png",
  "48": "img/icon48.png",
  "128": "img/icon.png"
},
"browser_action": {
  "default_icon": "img/icon.png"
},

"content_scripts": [
  {
    "run_at" :"document_end",
    "matches": ["https://www.skipthedishes.com/*/order"],
    "js": ["js/jquery-3.3.1.min.js","js/script.js"],
    "css":["css/style.css"]
  }
]
}